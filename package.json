{
  "name": "anoa-react-native-theme",
  "version": "0.1.0",
  "description": "Theme and stylesheet manager for ReactNative",
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "lint": "tslint 'src/**/*.ts{,x}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc -d --declarationDir ./typings",
    "build": "rm -rf typings && rm -rf dist && npm run compile && prettier ./typings/**/*.d.ts --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anoaland/anoa-react-native-theme.git"
  },
  "keywords": [
    "anoa",
    "react",
    "native",
    "theme"
  ],
  "author": "Budi Adiono <budiadiono@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anoaland/anoa-react-native-theme/issues"
  },
  "homepage": "https://github.com/anoaland/anoa-react-native-theme#readme",
  "dependencies": {
    "deepmerge": "^2.2.1",
    "react-native-extended-stylesheet": "^0.10.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "^16.7.1",
    "@types/react-native": "^0.57.7",
    "prettier": "^1.15.1",
    "react": "^16.6.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6"
  }
}
